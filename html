		<script type="text/javascript">
			var tags = document.getElementById("pics").getElementsByTagName("li");
			var cats = document.getElementById("shuzi").getElementsByTagName("span");
			function setNow(index){
				for(var i=0;i<tags.length;i++){
					tags[i].style.display="none";
					cats[i].className=" ";
				}
				tags[index].style.display="block";
				cats[index].className="on";
			}
			var current = 0;
			function setNext(){
				current++;
				if(current>=tags.length){
					current=0;
				}
				setNow(current);
			}
			var setNextInterval= setInterval("setNext()",2000);
			function setmouserOver(index1){
				clearInterval(setNextInterval);
				current=index1;
				setNow(current);
			}
		</script>
			
